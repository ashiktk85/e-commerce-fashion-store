      <script>
        let selectedAddress;

        function handleAddressSelection(checkbox) {
          if (checkbox.checked) {
            selectedAddress = checkbox.value;
          } else {
            selectedAddress = null;
          }
        }

        function placeOrder(cartid) {
          //    alert(cartid)
          //    alert(addressId)
          //    alert(checkedOption)
          const code = document.getElementById("code").value;

          var paymentMethod = document.querySelector(
            'input[name="payment_option"]:checked'
          ).value;

          const total = document.getElementById("total").innerHTML;
          // alert(total)

          $.ajax({
            url: "/placeOrder",
            method: "post",
            data: {
              selectedAddress,
              cartid,
              paymentMethod,
              total,
              code,
            },
            success: (response) => {
              if (response.status === true) {
                Swal.fire({
                  title: "Your order has been placed",
                  width: 600,
                  padding: "3em",
                  color: "#716add",
                  background: "#fff url(/images/trees.png)",
                  backdrop: `
                        rgba(0,0,123,0.4)
                        url("/images/nyan-cat.gif")
                        left top
                        no-repeat
                    `,
                  showConfirmButton: true,
                  confirmButtonColor: "#000000", // Set button color to black
                }).then((result) => {
                  if (result.isConfirmed) {
                    window.location.href = "/orderSuccess";
                  }
                });
              } else if (response.status == "fill") {
                Swal.fire({
                  icon: "error",
                  title: "Must fill the Options",
                  showConfirmButton: false,
                  timer: 1500,
                });
              } 
              // else if(response.status == "sizeErr") {
              //   Swal.fire({
              //     icon: "error",
              //     title: "Product out of stock",
              //     showConfirmButton: false,
              //     timer: 1500,
              //   });
              // }
               
              else if (response.status == "razorpay") {
                // alert(response.order.amount)
                // alert(response.order.id)
                // alert(response.order)

                let orderId = response.order.id;
                let address = response.address;

                let amount = response.total;
                let couponCode = response.code;

                var order_id = response.orderNumber;

                var options = {
                  key: "rzp_test_ECB3Zjd1NyXFF9", // Enter the Key ID generated from the Dashboard
                  amount: 50000, // Amount is in currency subunits. Default currency is INR. Hence, 50000 refers to 50000 paise
                  currency: "INR",
                  name: "Kevin Hills",
                  description: "Test Transaction",
                  image: "https://example.com/your_logo",
                  order_id: orderId, //This is a sample Order ID. Pass the `id` obtained in the response of Step 1
                  handler: function (response) {
                   

                    verifyPayment(
                      response,
                      orderId,
                      selectedAddress,
                      order_id,
                      amount,
                      couponCode
                    );
                  },
                  prefill: {
                    name: "Gaurav Kumar",
                    email: "gaurav.kumar@example.com",
                    contact: "9000090000",
                  },
                  notes: {
                    address: "Razorpay Corporate Office",
                  },
                  theme: {
                    color: "#000000",
                  },
                };

                var rzp = new Razorpay(options);
                rzp.on("payment.failed", function (response) {
                  //     console.log(couponCode)
                  //    console.log("hello");
                  //    console.log(address)
                  //    console.log(amount)

                  paymentFalid(selectedAddress, amount, couponCode);
                  rzp.close();
                });
                // alert(response.order.id)
                rzp.open();
              }  
            },
          });
        }

        function paymentFalid(selectedAddress, amount, couponCode) {
    $.ajax({
        url: "/paymentFaild",
        method: "post",
        data: {
            selectedAddress,
            amount,
            couponCode,
        },
        success: (response) => {
            if (response.status == "failed") {
               alert("Payment pending, order placed.");
               
                window.location.href = "/orderSuccess";
               
            }
        },
    });
}

function verifyPayment(payment, orderId, selectedAddress, order_id, amount, couponCode) {
    $.ajax({
        url: "/verifyPayment",
        method: "post",
        data: {
            payment,
            orderId,
            selectedAddress,
            order_id,
            amount,
            couponCode,
        },
        success: (response) => {
            if (response.status == true) {
                Swal.fire({
                    title: "Your order has been placed",
                    width: 600,
                    padding: "3em",
                    color: "#716add",
                    background: "#fff url(/images/trees.png)",
                    backdrop: `
                        rgba(0,0,123,0.4)
                        url("/images/nyan-cat.gif")
                        left top
                        no-repeat
                    `,
                    showConfirmButton: true,
                    confirmButtonColor: "#000000",
                }).then((result) => {
                    if (result.isConfirmed) {
                        window.location.href = "/orderSuccess";
                    }
                });
            }
        },
    });
}
q
        function apply(id) {
          const code = document.getElementById("code").value;
          const amount = document.getElementById("total").innerHTML;

          // alert(code)
          // alert(id)

          $.ajax({
            url: "/applyCoupon",
            method: "post",
            data: {
              code,
              id,
            },
            success: (response) => {
              if (response.status == true) {
                const Toast = Swal.mixin({
                  toast: true,
                  position: "bottom",
                  showConfirmButton: false,
                  timer: 1500,
                  // timerProgressBar: true,
                  didOpen: (toast) => {
                    toast.onmouseenter = Swal.stopTimer;
                    toast.onmouseleave = Swal.resumeTimer;
                  },
                });
                Toast.fire({
                  icon: "success",
                  title: "Coupon added",
                  position: "center",
                }).then((result) => {
                  document.getElementById("discount").innerHTML = parseInt(
                    response.total
                  );
                  document.getElementById("total").innerHTML =
                    parseInt(amount) - parseInt(response.total);
                  // document.getElementById("remove").style.display = "inline-block"
                  document.getElementById("apply").style.display = "none";
                  document.getElementById("code").style.display = "none";
                  // document.getElementById("text").innerHTML = "<h6>You already added this coupon code </h6> <h4 style='color:#007bff'>" + response.code + "</h4>"
                  // document.getElementById("show").style.display = "none"
                });
              } else if (response.status == "invalid") {
                Swal.fire({
                  icon: "error",
                  title: "Cannot find coupon.",
                  showConfirmButton: false,
                  timer: 1500,
                });
              } else if (response.status == "Limit") {
                Swal.fire({
                  icon: "error",
                  title: "Minimum Spending Required.",
                  showConfirmButton: false,
                  timer: 1500,
                });
              } else if (response.status == "used") {
                Swal.fire({
                  icon: "error",
                  title: "You have already used this coupon.",
                  showConfirmButton: false,
                  timer: 1500,
                });
              }
            },
          });
        }

        function remove() {
          const Toast = Swal.mixin({
            toast: true,
            position: "center",
            showConfirmButton: false,
            timer: 1500,
            // timerProgressBar: true,
            didOpen: (toast) => {
              toast.onmouseenter = Swal.stopTimer;
              toast.onmouseleave = Swal.resumeTimer;
            },
          });
          Toast.fire({
            icon: "success",
            title: "Coupon Removed",
          }).then((result) => {
            window.location.href = "/checkout";
          });
        }

        

      </script>

