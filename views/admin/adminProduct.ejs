        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Products</h2>
                    <!-- <p>Lorem ipsum dolor sit amet.</p> -->
                </div>
                <!-- <h5 style="margin-left: 650px;">Add a product</h5>  -->
                <div>
                    
                    <a href="/admin/addProduct" class="btn  btn-sm rounded" style="background-color: black; color: rgb(255, 255, 255);width: 150px;height: 38px;"><b>Add new Product +</b></a>
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="" style="width: 200px; height: 30px;margin-right: 100px;">
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="" name="catName" style="width: 150px; height: 30px;margin-right: 100px;" aria-placeholder="Catagory">
                                <% if(locals.catData){%>
                                    
                                <% catData.forEach(element =>{%>
                                
                                    <option value="<%=element._id%>"> <%= element.name %></option>
                                     <%})%>
                                     <%}%>
                            </select>
                        </div>
                        <div class="table-responsive">
                            <table class="table table-bordered table-sm">
                                <thead>
                                    <tr>
                                        <th class="text-center">Image</th>
                                        <th class="text-center">Name</th>
                                        <th class="text-center">Regular price</th>
                                        <th class="text-center">Offer Price</th>
                                        <th class="text-center">Action</th>
                                        <th class="text-center">Product Details</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for (let i = 0; i < proData.length; i++) { %>
                                        <tr>
                                            <td class="align-middle text-center" style="width: 100px;">
                                                <img style="width: 100%;" src="/uploads/<%= proData[i].image[0] %>" alt="Product">
                                            </td>
                                            <td class="align-middle text-center">
                                                <a href="#" class="text-truncate"><%= proData[i].name %></a>
                                            </td>
                                            <td class="align-middle text-center">
                                                $<%= proData[i].regularPrice %>
                                            </td>
                                            <td class="align-middle text-center">
                                                <% if (proData[i].offerPrice === null) { %>
                                                 <span>Nill</span>
                                                <% } else { %>
                                                $<%= proData[i].offerPrice %>
                                                <% } %>
                                            </td>
                                            <td class="align-middle text-center">
                                                <% if (proData[i].is_blocked == true) { %>
                                                    <a href="/admin/block-pro?id=<%= proData[i]._id %>" class="btn btn-sm font-sm btn-light rounded unlist-btn">
                                                        <i class="material-icons md-delete_forever"></i> Unlist
                                                    </a>
                                                <% } else { %>
                                                    <a href="/admin/edit-pro?id=<%= proData[i]._id %>" class="btn btn-sm font-sm rounded btn-brand">
                                                        <i class="material-icons md-edit"></i> Edit
                                                    </a>
                                                    <a href="/admin/unblock-pro?id=<%= proData[i]._id %>" class="btn btn-sm font-sm btn-light rounded">
                                                        <i class="material-icons md-delete_forever"></i> List
                                                    </a>
                                                <% } %>
                                            </td>
                                            <td class="align-middle text-center">
                                                <a href="/admin/edit-detailedView?id=<%= proData[i]._id %>" class="btn btn-sm font-sm rounded btn-brand">
                                                    <i class="material-icons"></i> View
                                                </a>
                                            </td>
                                        </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                        
                        
                        
                        
                    </div>
                </header> <!-- card-header end// -->
                 <!-- card end// -->
     
        </section>
