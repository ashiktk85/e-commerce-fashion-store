        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Coupon</h2>
                    <p>Add, edit or delete a Coupon</p>
                </div>
                <div>
                    <input type="text" placeholder="Search Coupon" class="form-control bg-white">
                </div>
            </div>
            <div class="d-flex justify-content-end pb-20">
                <a href="/admin/addCoupon" class="btn btn-sm btn-primary rounded font-sm" style="background-color: #000000;">Create Coupon</a>
            </div>
            <div class="card">
                <div class="card-body">
             
                        <div class="col-md-15">
                            <div class="table-responsive">
                                <table class="table table-hover">
                                    <thead>
                                        <tr style="width: 100%;">
                                          
                                            <th>Code</th>
                                            <th>Name</th>
                                            <th>ST-Date</th>
                                            <th>ED-Date</th>
                                            <th>Min-amount</th>
                                            <th>Max-amount</th>
                                            <th>Status</th>
                                            <th class="text-end" >Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                      <% if (couponData.length>0) { %>

                                        <% for( let i = 0; i < couponData.length; i++ ) { %>

                                            <tr>
                                            
                                                <td><%= couponData[i].couponCode  %></td>
                                                <td><b><%= couponData[i].name  %></b></td>
                                                <td><%= couponData[i].startDate %></td>
                                                <td><%=  couponData[i].EndDate %></td>
                                                <td><%=  couponData[i].minimumAmount %></td>
                                                <td><%=  couponData[i].maximumAmount%></td>
                                                <% if (couponData[i].isActive==true) { %>

                                                    <td id="status<%=i%>">Active</td>
                                                    <td class="text-end" style="text-align: right;">
                                                        <a href="/admin/coupon-edit?id=<%=couponData[i]._id  %>" id="edit<%=i%>" class="btn btn-info rounded font-sm mt-6 p-10" style="display: inline-block; margin-right: 10px;background-color: #000000;color: #ffffff;border: #000000;">EDIT</a>
                                                        <button type="button" class="btn btn-primary" style="padding: 10px; display: inline-block;" id="listButton<%=i%>" onclick="Block('<%= couponData[i]._id %>','<%=i%>')">Not Active</button>
                                                      </td>
                                                 
                                                <% }else{ %>
                                                    <td id="status<%=i%>">Not Active</td>
                                                    <td class="text-end">
                                                        <a href="/admin/coupon-edit?id=<%= couponData[i]._id %>" id="edit<%=i%>" class="btn  rounded font-sm mt-6 p-10" style="display: none; margin-right: 10px;background-color: #000000;color: #ffffff;">EDIT</a>
                                                        <button type="button" class="btn btn-primary" style="padding: 10px; display: inline-block;" id="listButton<%=i%>" onclick="Block('<%=couponData[i]._id %>','<%=i%>')">Active</button>
                                                    </td>
                                                    <% } %>
                                               
                     
                                            </tr>

                                        
                                        <% } %>
                                       
                                      <% }else{ %>

                                        <tr>
                                            <td>
                                                No Coupon
                                            </td>
                                        </tr>

                                        <% } %>
                                                
                                        
                                          
                                    </tbody>
                                </table>
                            </div>
                        </div> 
                
                </div> 
            </div>
        </section> 
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js"
        integrity="sha512-v2CJ7UaYy4JwqLDIrZUI/4hqeoQieOmAZNXBeQyjo21dadnwR+8ZaIJVT8EE2iyI61OV8e6M8PP2/4hpQINQ/g=="
         crossorigin="anonymous" referrerpolicy="no-referrer"></script>
     <script>
       function Block(id,index){
           // alert(id)
         $.ajax({
           url:"/admin/coupon-block",
           method:"post",
           data:{
               id
           },
           success:(response)=>{
               if(response.status==true){
                   // alert("hello")
                   document.getElementById("edit"+index).style.display="none"
                       document.getElementById("listButton"+index).innerHTML='Active'
                       document.getElementById('status'+index).innerHTML='Not Active'
               }else if(response.status==false){
                  
                       document.getElementById("edit"+index).style.display="inline-block"
                       document.getElementById("listButton"+index).innerHTML='Not Active'
                       document.getElementById('status'+index).innerHTML='Active'
               }
           }
         })
       }
     </script>