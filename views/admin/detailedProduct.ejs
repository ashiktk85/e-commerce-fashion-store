        <div class="product-details-wrapper pt-100 pb-100" >
            <div class="container" >
                <h2 >Detailed product view</h2>
                <div class="row">
                    <div class="col-lg-12" style="color: #000000;">
                        <!-- Single Product Card -->
                        <div class="card" style="background-color: #c9c8c8;">
                            <div class="card-body">
                                <!-- Product Images and Details Row -->
                                <div class="row">
                                    <div class="col-lg-6">
                                        <!-- Product Images -->
                                        <div class="product-images">
                                            <% for (let i = 0; i < Math.min(proData.image.length, 5); i++) { %>
                                                <div class="product-image">
                                                    <img class="img-fluid" src="/uploads/<%= proData.image[i] %>"
                                                        alt="Product Image">
                                                </div>
                                            <% } %>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <!-- Product Details Table -->
                                        <table class="table table-bordered">
                                            <tbody>
                                                <tr>
                                                    <th scope="row">Name</th>
                                                    <td><%= proData.name %></td>
                                                </tr>
                                                <tr>
                                                    <th scope="row">Description</th>
                                                    <td>
                                                        <%= proData.discripiton %>
                                                    </td>
                                                </tr>
                                                
                                                <tr>
                                                    <th scope="row">Regular Price</th>
                                                    <td><%= proData.regularPrice %></td>
                                                </tr>
                                                <tr>
                                                    <th scope="row">Offer Price</th>
                                                    <td><%= proData.offerPrice !== null ? '$' + proData.offerPrice : 'Nill' %></td>
                                                </tr>
                                                <tr>
                                                    <th scope="row">Stock</th>
                                                    <td>
                                                        <p style="color: #000000;">Small: <%= proData.size.s.quantity %></p>
                                                        <p style="color: #000000;">Medium: <%= proData.size.m.quantity %></p>
                                                        <p style="color: #000000;">Large: <%= proData.size.l.quantity %></p>
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <th scope="row">Category</th>
                                                    <td><%= catName %></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                        <!-- End Product Details Table -->
                                    </div>
                                </div>
                                <!-- End Product Images and Details Row -->
                            </div>
                        </div>
                        <!-- End Single Product Card -->
                    </div>
                </div>
