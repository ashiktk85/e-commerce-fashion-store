                        <script>
                            function previewImages() {
                                var input = document.getElementById('Pro_img');
                                var previewContainer = document.getElementById('imagePreviewContainer');
                                var defaultImage = document.getElementById('defaultImage');
                        
                                previewContainer.innerHTML = ''; // Clear previous preview
                        
                                if (input.files && input.files.length > 0) {
                                    Array.from(input.files).forEach(file => {
                                        var reader = new FileReader();
                        
                                        reader.onload = function (e) {
                                            var img = document.createElement('img');
                                            img.src = e.target.result;
                                            img.alt = 'product image';
                                            img.style.width = '100px'; // Set width as needed
                                            img.style.height = '100px'; // Set height as needed
                                            previewContainer.appendChild(img);
                                        };
                        
                                        reader.readAsDataURL(file);
                                    });
                                } else {
                                    // If no file selected, show default image
                                    var img = document.createElement('img');
                                    img.src = defaultImage.src;
                                    img.alt = 'default product image';
                                    img.style.width = '100px'; // Set width as needed
                                    img.style.height = '100px'; // Set height as needed
                                    img.style.marginRight = '5px';
                                    previewContainer.appendChild(img);
                                }
                            }
                        </script>
                        
                        
